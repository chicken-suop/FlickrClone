    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
